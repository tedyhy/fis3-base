{
  "name": "fis3-base",
  "version": "0.1.0",
  "description": "基于fis3的纯前端解决方案，拿来即用的fis3脚手架。",
  "main": "fis-conf.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "generate-env": "node ./setup-env.js",
    "prebuild": "npm run clean && npm run generate-env",
    "build": "fis3 release prod --verbose -lc",
    "postbuild": "sh ./to-dist.sh",
    "buildBeta": "npm run clean && cross-env NODE_ENV=beta npm run build",
    "prestart": "npm run clean && cross-env NODE_ENV=development npm run generate-env",
    "start": "nodemon ./bin/www && fis3 release --verbose -wcL",
    "start:prod": "node ./dist/bin/www"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yanhaijing/fis3-base.git"
  },
  "author": {
    "name": "yanhaijing",
    "email": "yanhaijing@yeah.net",
    "url": "http://yanhaijing.com"
  },
  "license": [
    {
      "type": "MIT",
      "url": "https://github.com/yanhaijing/fis3-base/blob/master/MIT-LICENSE.txt"
    }
  ],
  "bugs": {
    "url": "https://github.com/yanhaijing/fis3-base/issues"
  },
  "homepage": "https://github.com/yanhaijing/fis3-base",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "ip": "^1.1.5",
    "moment": "^2.21.0",
    "normalize.css": "^3.0.3",
    "on-finished": "^2.3.0",
    "perfect-css": "^0.3.5",
    "process": "^0.11.10",
    "redux": "^3.7.2",
    "sammy": "^0.7.6",
    "underscore": "^1.8.3",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2"
  },
  "devDependencies": {
    "cross-env": "5.1.3",
    "nodemon": "1.17.1",
    "rimraf": "2.6.2"
  }
}
